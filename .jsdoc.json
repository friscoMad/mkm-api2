{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|_old|dist|examples)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
        "includeDate": false
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}
