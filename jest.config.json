{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "src/**/*.js",
        "!src/index.js",
        "!src/global.js"
    ],
    "coverageReporters": ["lcov", "text", "text-summary"],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setupTest.js",
    "setupFiles": ["<rootDir>/jest/setupEnv.js"]
}
